{
	"name": "Copy Only Text",
	"manifest_version": 3,
	"version": "1.0",
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"permissions": ["activeTab","scripting"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {},
	"commands": {
	  "_execute_action": {
		"suggested_key": {
		  "default": "Ctrl+C",
		  "mac": "MacCtrl+C"
		},
		"description": "Run extension."
	  }
	}
}